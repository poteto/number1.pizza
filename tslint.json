{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [true, { "singleQuote": true }],
    "import-name": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-var-requires": false
  }
}